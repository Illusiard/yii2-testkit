{
  "name": "illusiard/yii2-testkit",
  "type": "library",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "codeception/module-yii2": "*",
    "yiisoft/yii2": "^2.0",
    "codeception/module-asserts": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Illusiard\\Yii2Testkit\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
